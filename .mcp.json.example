{
  "mcpServers": {
    "n8n-mcp": {
      "command": "/path/to/project/.claude/scripts/run-n8n-mcp.sh",
      "args": [],
      "env": {
        "CLAUDE_PROJECT_DIR": "/path/to/project"
      }
    },
    "n8n-instance-mcp": {
      "type": "http",
      "url": "http://localhost:5678/mcp-server/http",
      "headers": {
        "Authorization": "Bearer your-mcp-server-token"
      }
    }
  }
}
