# n8n MCP Server Connection
# Public URL for MCP server to connect to n8n (via ngrok)
N8N_URL=https://your-ngrok-url.ngrok-free.app

# Deployment Path
# Path to the actual n8n Docker deployment
N8N_DEPLOYMENT_PATH=/path/to/your/n8n/deployment

# n8n Database Credentials (for direct database access)
N8N_DB_HOST=postgres
N8N_DB_PORT=5432
N8N_DB_NAME=n8n
N8N_DB_USERNAME=your-n8n-db-username
N8N_DB_PASSWORD=your-n8n-db-password

# n8n Critical Configuration
N8N_ENCRYPTION_KEY=your-n8n-encryption-key

# n8n API Key
N8N_API_KEY=grab-api-key-from-n8n-ui
