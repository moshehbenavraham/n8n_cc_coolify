# Server Identity
SERVER_NAME=your-server-hostname
SERVER_IP=your-server-ip

# Container/Network IDs (change on redeployment)
N8N_NETWORK_ID=your-n8n-network-id
AGENT_MEMORY_CONTAINER_ID=your-agent-memory-container-id

# Service URLs
COOLIFY_URL=https://coolify.example.com
N8N_URL=https://n8n.example.com

# Deployment Path (path to actual n8n Docker deployment)
N8N_DEPLOYMENT_PATH=/path/to/n8n

# Workflow IDs
ATLAS_WORKFLOW_ID=your-atlas-workflow-id

# Coolify Credentials
COOLIFY_DB_USERNAME=coolify
COOLIFY_DB_PASSWORD=your-coolify-db-password
COOLIFY_API_TOKEN=your-coolify-api-token
COOLIFY_API_URL=https://coolify.example.com/api/v1

# n8n Database Credentials
N8N_DB_HOST=postgres
N8N_DB_PORT=5432
N8N_DB_NAME=n8n
N8N_DB_USERNAME=your-n8n-db-username
N8N_DB_PASSWORD=your-n8n-db-password

# n8n Critical Configuration
N8N_ENCRYPTION_KEY=your-n8n-encryption-key
N8N_RUNNERS_AUTH_TOKEN=your-n8n-runners-auth-token

# n8n API Key
N8N_API_KEY=your-n8n-api-key

# Agent Memory PostgreSQL Credentials
AGENT_MEMORY_DB_HOST=container-id-or-hostname
AGENT_MEMORY_DB_PORT_EXTERNAL=5400
AGENT_MEMORY_DB_PORT_INTERNAL=5432
AGENT_MEMORY_DB_NAME=postgres
AGENT_MEMORY_DB_USERNAME=postgres
AGENT_MEMORY_DB_PASSWORD=your-agent-memory-db-password

# n8n Instance Level MCP URL
N8N_INSTANCE_LEVEL_MCP_URL=http://localhost:5678/mcp-server/http

# n8n Instance Level MCP Token
N8N_INSTANCE_LEVEL_MCP_TOKEN=your-mcp-server-token
